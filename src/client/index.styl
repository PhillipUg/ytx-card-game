*
    box-sizing: border-box   
body, html
    font-family: sans-serif
    margin: 0
    width: 100vw;
#root
    height: 100vh;
    width: 100vw;
.noselect
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none; 
